<h1>lista de productos</h1>
{{#if products}}
<section>
    {{#each products}}
    <ul>
        <li>nombre:{{this.title}}</li>
        <li>precio:{{this.price}}</li>
        <li>stock:{{this.stock}}</li>
    </ul>
    <button class="agregarCarrito" type="submit" data-id="{{this._id}}">Agregar al Carrito</button>
    {{/each}}
</section>

{{/if}}
{{#else}}
<h2>a√∫n no tenes productos</h2>
{{/else}}

<script src="../javascript/main.js"></script>